{
  "name": "mf-react-basketball-logos",
  "version": "1.0.0",
  "description": "Unofficial React components for basketball team logos (NBA, EuroLeague, etc.). Modern maintained fork.",
  "author": "Mattia Ferrari",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattiaferrarii/mf-react-basketball-logos.git"
  },
  "bugs": {
    "url": "https://github.com/mattiaferrarii/mf-react-basketball-logos/issues"
  },
  "homepage": "https://github.com/mattiaferrarii/mf-react-basketball-logos#readme",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./nba/*": {
      "types": "./dist/nba/*.d.ts",
      "import": "./dist/nba/*.mjs",
      "require": "./dist/nba/*.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run typecheck && npm run test && npm run build"
  },
  "keywords": [
    "react",
    "basketball",
    "logos",
    "nba",
    "euroleague",
    "lba",
    "svg",
    "typescript",
    "icons",
    "components"
  ],
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/ui": "^2.1.0",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.4.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
